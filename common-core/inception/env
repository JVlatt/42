# MYSQL SETUP
MYSQL_DB=mmanuell_db # database name
MYSQL_USER=mmanuell # database user
MYSQL_PASSWORD=totosql # database user password
MYSQL_ROOT_PASSWORD=totosqlroot # root password

# WP SETUP
DOMAIN_NAME=mmanuell.fr # domain name of the website
WP_ADMIN_N=mmanuelladmin # admin name
WP_ADMIN_P=totowpadmin # admin password
WP_TITLE=mmanuell_wp_title # website title
WP_ADMIN_E=mmanuelladmin@contact.com # admin email

# WP NEW USER SETUP
WP_U_NAME=mmanuell # user name of the new user
WP_U_PASS=totowp # user password
WP_U_ROLE=author # author, editor, Administrator ...
WP_U_EMAIL=mmanuell@contact.com # user email
